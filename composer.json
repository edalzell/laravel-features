{
    "name": "silentz/laravel-features",
    "description": "This is my package laravel-features",
    "license": "MIT",
    "keywords": [
        "Silent Z",
        "laravel",
        "laravel-features"
    ],
    "authors": [
        {
            "name": "Erin Dalzell",
            "email": "erin@silentz.co",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/silentzco/laravel-features",
    "require": {
        "php": "^8.3",
        "composer/composer": "^2.8",
        "illuminate/contracts": "^11.0 || ^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^9.0.0 || ^10.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.40"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": ["src/Composer/GenerateNamespaces.php"],
        "psr-4": {
            "SilentZ\\Features\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilentZ\\Features\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "class": "SilentZ\\Features\\Composer\\GenerateNamespaces",
        "laravel": {
            "aliases": {
                "Features": "SilentZ\\Features\\Facades\\Features"
            },
            "providers": [
                "SilentZ\\Features\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
